{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "packageurl": {
      "defaultValue": "https://templatedeployments.blob.core.windows.net/test/Contoso2.zip",
      "type": "string"
    },
    "dbLogin": {
      "defaultValue": "SQLadmin",
      "type": "string"
    },
    "dbPassword": {
      "defaultValue": "Password1234!",
      "type": "string"
    },
    "BacpacUrl": {
      "defaultValue": "https://templatedeployments.blob.core.windows.net/test/ContosoDB.bacpac",
      "type": "string"
    }
  },
  "variables": {
    "SQLserver": "[concat('sql',variables('guid'))]",
    "SQLdatabase": "[concat('db',variables('guid'))]",
    "location": "[resourceGroup().location]",
    "guid": "[uniqueString(resourceGroup().id)]"
  },
  "resources": [
    {
      "name": "[variables('SQLserver')]",
      "type": "Microsoft.Sql/servers",
      "location": "[variables('location')]",
      "apiVersion": "2014-04-01-preview",
      "properties": {
        "administratorLogin": "[parameters('dbLogin')]",
        "administratorLoginPassword": "[parameters('dbPassword')]",
        "version": "12.0"
      },
      "resources": [
        {
          "type": "firewallrules",
          "apiVersion": "2014-04-01-preview",
          "properties": {
            "endIpAddress": "0.0.0.0",
            "startIpAddress": "0.0.0.0"
          },
          "name": "AllowAllAzureIps",
          "dependsOn": [
            "[variables('SQLserver')]"
          ]
        },
        {
          "apiVersion": "2014-04-01-preview",
          "dependsOn": [
            "[resourceId('Microsoft.Sql/servers', variables('SQLserver'))]"
          ],
          "location": "[variables('location')]",
          "name": "[variables('SQLdatabase')]",
          "properties": {
            "edition": "Basic",
            "maxSizeBytes": "1073741824",
            "requestedServiceObjectiveName": "Basic"
          },
          "resources": [
            {
              "name": "IMPORTNAME",
              "type": "extensions",
              "apiVersion": "2014-04-01-preview",
              "dependsOn": [
                "[resourceId('Microsoft.Sql/servers/databases', variables('SQLserver'), variables('SQLdatabase'))]"
              ],
              "tags": {
                "displayName": "IMPORTNAME"
              },
              "properties": {
                "storageKeyType": "StorageAccessKey",
                "storageKey": "3o7SDiEAev4eRYRlPnsGbX89jw/pR2DPHJu85BPPx3JnaqLbrAO3RrWqnluQXTzl6flJnRlW8Zr5Wwn0BIvsqQ==",
                "storageUri": "[parameters('BacpacUrl')]",
                "administratorLogin": "[parameters('dbLogin')]",
                "administratorLoginPassword": "[parameters('dbPassword')]",
                "operationMode": "Import"
              }
            }
          ],
          "tags": {
            "displayName": "DBNAME"
          },
          "type": "databases"
        }
      ]
    },
    {
      "type": "Microsoft.Web/serverfarms",
      "sku": {
        "name": "F1",
        "tier": "Free",
        "size": "F1",
        "family": "F",
        "capacity": 0
      },
      "name": "[variables('guid')]",
      "apiVersion": "2015-08-01",
      "location": "[variables('location')]",
      "properties": {
        "name": "[variables('guid')]",
        "numberOfWorkers": 0
      },
      "resources": [],
      "dependsOn": []
    },
    {
      "type": "Microsoft.Web/sites",
      "name": "[variables('guid')]",
      "apiVersion": "2015-08-01",
      "location": "[variables('location')]",
      "properties": {
        "name": "[variables('guid')]",
        "hostNames": [
          "[concat(variables('guid'),'.azurewebsites.net')]"
        ],
        "enabledHostNames": [
          "[concat(variables('guid'),'.azurewebsites.net')]",
          "[concat(variables('guid'),'.azurewebsites.net')]"
        ],
        "hostNameSslStates": [
          {
            "name": "[concat(variables('guid'),'.azurewebsites.net')]",
            "sslState": 0,
            "thumbprint": null,
            "ipBasedSslState": 0
          },
          {
            "name": "[concat(variables('guid'),'.scm.azurewebsites.net')]",
            "sslState": 0,
            "thumbprint": null,
            "ipBasedSslState": 0
          }
        ],
        "serverFarmId": "[resourceId('Microsoft.Web/serverfarms', variables('guid'))]",
        "siteConfig": {
          "connectionStrings": [
            {
              "name": "ContosoUniversityDataEntities",
              "connectionString": "[concat('metadata=res://*/Models.ContosoModel.csdl|res://*/Models.ContosoModel.ssdl|res://*/Models.ContosoModel.msl;provider=System.Data.SqlClient;provider connection string=\";Data Source=',variables('SQLserver'),'.database.windows.net',';Initial Catalog=',variables('SQLdatabase'),';Persist Security Info=True;User ID=',parameters('dbLogin'),';Password=',parameters('dbPassword'),'\";')]",
              "type": 3
            }
          ]
        }
      },
      "resources": [
        {
          "apiVersion": "2015-08-01",
          "dependsOn": [
            "[concat('Microsoft.Web/Sites/', variables('guid'))]"
          ],
          "name": "MSDeploy",
          "properties": {
            "packageUri": "[parameters('packageurl')]",
            "dbType": "None",
            "connectionString": "",
            "setParameters": {}
          },
          "type": "extensions"
        }
      ],
      "dependsOn": [
        "[resourceId('Microsoft.Web/serverfarms', variables('guid'))]"
      ]
    }
  ]
}
